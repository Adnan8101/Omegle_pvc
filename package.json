{
  "name": "inef_pvc",
  "version": "1.0.0",
  "description": "Advanced Private Voice Channel Generator System for Discord",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "register": "ts-node src/deploy-commands.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:check": "node scripts/check-db-connections.js",
    "db:cleanup": "echo '⚠️  This will kill idle database connections. Proceed?' && psql $DATABASE_URL -f scripts/cleanup-db-connections.sql",
    "test:vcns": "ts-node tools/vcns-test/simulate-vc-storm.ts",
    "test:full": "ts-node tools/vcns-test/full-system-test.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "voice-channel",
    "pvc"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "canvas": "^3.2.1",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "moment-timezone": "^0.6.0"
  },
  "devDependencies": {
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^25.0.9",
    "nodemon": "^3.1.11",
    "pg": "^8.18.0",
    "prisma": "^6.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
